{
  "name": "github-stats",
  "version": "0.1.0",
  "private": false,
  "homepage": "https://spread-the-code.github.io/github-stats",
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "4.2.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "highcharts": "^8.1.2",
    "highcharts-react-official": "^3.0.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "4.2.2",
    "react-scripts": "3.4.0",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "BROWSER=\"none\" react-scripts start",
    "build": "PUBLIC_URL=\"./\" react-scripts build",
    "test": "BROWSER=\"none\" react-scripts test",
    "test:coverage": "BROWSER=\"none\" react-scripts test --coverage",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy:now": "now ./build --token=$NOW_TOKEN",
    "deploy:github": "npm run build && gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "enzyme-to-json": "^3.4.4",
    "gh-pages": "^3.0.0",
    "now": "^17.0.4",
    "ts-jest": "^25.2.1"
  }
}
